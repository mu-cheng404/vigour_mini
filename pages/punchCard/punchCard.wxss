.page {
  width: 750rpx;
  height: 100vh;
  border: 1rpx solid rgb(247, 247, 247);
  /* background-color: teal; */
}

.punchCategoryArray {
  width: 100%;
  height: 394rpx;
}

.categoryBox {
  width: 150rpx;
  height: 170rpx;
  background-color: rgb(239, 239, 248);
  margin: 18rpx;
  border-radius: 15rpx;
  float: left;
}

.box_image {
  width: 150rpx;
  height: 130rpx;
  border-radius: 15rpx;
  /* background-color: rgb(46, 77, 46); */
  display: flex;
  justify-content: center;
  align-items: center;
}

.box_image image {
  width: 100rpx;
  height: 100rpx;
}

.categoryText {
  /* font-weight: bold; */
  border-bottom-right-radius: 10rpx;
  border-bottom-left-radius: 10rpx;
  font-size: 30rpx;
  line-height: 40rpx;
  text-align: center;
  color: rgb(105, 117, 112);
}

.supervise_interact {
  width: 100%;
  height: 65rpx;
  /* background-color: violet; */
  clear: both;
  display: flex;
  margin-top: 80rpx;
}

.text1 {
  width: 200rpx;
  height: 65rpx;
  /* background-color: turquoise; */
  line-height: 65rpx;
  text-align: center;
  font-weight: bold;
}

.text2 {
  width: 300rpx;
  height: 65rpx;
  /* background-color: yellow; */
  line-height: 65rpx;
  font-size: 20rpx;
  color: rgb(112, 105, 105);
  /* text-align: center; */
}

.superviseIcons {
  width: 250;
  display: flex;
  justify-content: center;
  align-items: center;
}

.superviseIcons image {
  width: 50rpx;
  height: 50rpx;
  /* border-radius: 50%; */
  margin-left: 10rpx;
}

.textDisplay {
  width: 100%;

  color: rgb(199, 190, 190);
  font-size: 30rpx;
  text-align: center;
  margin-top: 200rpx;
}
