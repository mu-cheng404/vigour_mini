.header{
  width: 100%;
  height: 100rpx;
}
.add{
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 18px;
  text-align: center;
  border-radius: 50rpx;
  position: fixed;
  right: 20rpx;
  top: 20rpx;
  background-color: white;
}
.planBox{
  width: 710rpx;
  height: 100rpx;
  margin: 20rpx;
  display: flex;
  border-radius: 20rpx;
  position: relative;
}
.label{
  width: 100rpx;
  height: inherit;
  line-height: 100rpx;
  text-align: center;
}

.plan{
  height: inherit;
  width: 300rpx;
  line-height: 100rpx;
  padding-left: 20rpx;
  margin-left: 20px;
}
.model{
  width: 150rpx;
  height: inherit;
  line-height: 100rpx;
}
.punch_button{
  
  position: absolute;
  right: 20px;
  height: inherit;
  line-height: inherit;
}